{
  "expo": {
    "name": "ihelp",
    "slug": "ihelp",
    "version": "1.0.0",
    "plugins": [
      [
         "expo-notifications",
         {
             "icon": "./assets/ihelp/logo.png",
             "color": "#ffffff"
         }
      ]
   ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "mn.ihelp.hr",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "versionCode": 1,
      "package": "mn.ihelp.hr",
      "permissions": [],
      "googleServicesFile": "./GoogleService-Info.plist",
      "useNextNotificationsApi": true
    }
   
  }
}
